<h1> New Student </h1>


<%= form_with model: @student do |f| %>

    <%= f.label :username %>
    <%= f.text_field :username %>

    <br><br>

    <%= f.label :major %>
    <%= f.text_field :major %>

    <br><br>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <br><br>

    <%= f.submit "SignUP" %>


<% end %>
