


<h1>Note SHow Page</h1>





<h2><%= @note.title %></h2>
<h3><%= @note.content%></h3>


<%= binding.pry%>

<% if @note.student_id == current_student.id %>
    edit: <%= link_to "Edit", edit_note_path(@note)%>
    delete: <%= link_to "Delete Note", note_path(@note), method: :delete, data: { confirm: 'Are you sure?' } %>
<% else %>
    <%= link_to "All Notes", notes_path %>
<% end %>


<%= link_to "Add This Note To A Classroom", new_classroom_note_path %>